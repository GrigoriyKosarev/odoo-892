<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_export_bill_wizard" model="ir.ui.view">
        <field name="name">export.bill.wizard.form</field>
        <field name="model">bio.export.bill.wizard</field>
        <field name="arch" type="xml">
            <form string="Bill report HU">
                <group>
                    <field name="move_ids" widget="many2many_tags"/>
                </group>
                <footer>
                    <button name="action_export_excel"
                            type="object"
                            string="Export"
                            class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>